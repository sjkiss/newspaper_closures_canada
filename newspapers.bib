@article{ellisMeaningfulConsiderationReview2005,
  title = {Meaningful Consideration? {{A}} Review of Traditional Knowledge in Environmental Decision Making},
  author = {Ellis, S},
  date = {2005-02-28},
  journaltitle = {Arctic},
