@article{nyhanTakingFactChecksLiterally2020,
  title = {Taking {{Fact-Checks Literally But Not Seriously}}? {{The Effects}} of {{Journalistic Fact-Checking}} on {{Factual Beliefs}} and {{Candidate Favorability}}},
