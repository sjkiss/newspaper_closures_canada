@article{nykiforukSmokefreeSpacesTime2008,
  title = {Smoke-Free Spaces over Time: A Policy Diffusion Study of Bylaw Development in {{Alberta}} and {{Ontario}}, {{Canada}}},
  shorttitle = {Smoke-Free Spaces over Time},
  author = {Nykiforuk, Candace I. J. and Eyles, John and Campbell, H. Sharon},
  date = {2008},
  journaltitle = {Health \& Social Care in the Community},
  volume = {16},
