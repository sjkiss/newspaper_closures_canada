@article{wyniamkMandatingVaccinationWhat2007,
  title = {Mandating Vaccination: What Counts as a 'mandate' in Public Health and When Should They Be Used?},
  shorttitle = {Mandating Vaccination},
  author = {{Wynia MK}},
  date = {2007-12},
  journaltitle = {American Journal of Bioethics},
