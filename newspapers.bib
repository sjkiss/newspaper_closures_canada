@article{krosnickComparisonsPartyIdentification1993,
  title = {Comparisons of {{Party Identification}} and {{Policy Preferences}}: {{The Impact}} of {{Survey Question Format}}},
  author = {Krosnick, Jon A and Berent, Matthew K},
  date = {1993-08-01},
  journaltitle = {American Journal of Political Science},
  shortjournal = {American Journal of Political Science},
  volume = {37},
  number = {3},
  pages = {941--964},
