@article{stahlHOLOCENEBIODIVERSITYArchaeological1996a,
  title = {{{HOLOCENE BIODIVERSITY}}: {{An Archaeological Perspective}} from the {{Americas}}},
  shorttitle = {{{HOLOCENE BIODIVERSITY}}},
  author = {Stahl, Peter W.},
  date = {1996},
