@article{pattPerceptionsCommunicationStrategies2014,
  title = {Perceptions and Communication Strategies for the Many Uncertainties Relevant for Climate Policy},
  author = {Patt, Anthony G. and Weber, Elke U.},
  date = {2014},
  journaltitle = {Wiley Interdisciplinary Reviews: Climate Change},
  volume = {5},
