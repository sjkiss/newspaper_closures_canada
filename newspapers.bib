@article{salmelaEmotionalRootsRightwing2017,
  title = {Emotional Roots of Right-Wing Political Populism},
  author = {Salmela, Mikko and von Scheve, Christian},
  options = {useprefix=true},
  date = {2017-12-01},
  journaltitle = {Social Science Information},
