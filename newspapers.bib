  author = {Tyl, R W and Myers, C B and Marr, M C and Thomas, B F and Keimowitz, A R and Brine, D R and Veselica, M M and Fail, P A and Chang, T Y and Seely, J C and Joiner, R L and Butala, J H and Dimond, S S and Cagen, S Z and Shiotsuka, R N and Stropp, G D and Waechter, J M},
  date = {2002-06-30},
  journaltitle = {Toxicological sciences : an official journal of the Society of Toxicology},
