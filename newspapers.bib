@article{heathThorsteinVeblenAmerican2008,
  title = {Thorstein {{Veblen}} and {{American Social Criticism}}},
  author = {Heath, Joseph},
  date = {2008-09-25},
  journaltitle = {The Oxford Handbook of American Philosophy},
  doi = {10.1093/oxfordhb/9780199219315.003.0013},
  url = {http://www.oxfordhandbooks.com/view/10.1093/oxfordhb/9780199219315.001.0001/oxfordhb-9780199219315-e-13},
  urldate = {2018-09-20},
  abstract = {This article aims to identify and explain some of the characteristic features of the style of social criticism that Veblen pioneered. In order to do so, it first sets aside several of the misconceptions that have arisen about his work. The first concerns its generality. Veblen must be understood not merely as a critic of an obsolete “aristocratic” pattern of upper-class consumption, but rather as the progenitor of a general theory of the relationship between class, status, private property, and social inequality. The second major misunderstanding of Veblen's work arises from the assumption that he is engaged in moralizing social criticism. On the contrary, Veblen shared with both Marx and Freud the desire to refrain from making simple value judgments.},
  langid = {english},
