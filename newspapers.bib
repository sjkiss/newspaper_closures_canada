@article{calpenaInfluenceFormulationVitro1999,
  title = {Influence of the Formulation on the in Vitro Transdermal Penetration of Sodium Diclofenac - {{Evaluation}} of the Topical and Systemic Anti-Inflammatory Activity in the Rat},
  author = {Calpena, A and Escribano, E and San Martin, H and Lauroba, J and Obach, R and Domenech, J},
  date = {1999-11-30},
