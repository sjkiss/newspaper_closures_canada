@article{zallerMonicaLewinskyContribution1998,
  title = {Monica {{Lewinsky}}'s {{Contribution}} to {{Political Science}}*},
  author = {Zaller, John R.},
  date = {1998-06},
  journaltitle = {PS: Political Science \& Politics},
  volume = {31},
  number = {2},
  pages = {182--189},
