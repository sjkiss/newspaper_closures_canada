  langid = {english},
  pagetotal = {308},
  keywords = {Social Science / Ethnic Studies / American / Native American Studies}
}

@book{aschBeingHereStay2014,
  title = {On {{Being Here}} to {{Stay}}: {{Treaties}} and {{Aboriginal Rights}} in {{Canada}}},
  shorttitle = {On {{Being Here}} to {{Stay}}},
  author = {Asch, Michael},
  date = {2014-01-01},
  eprint = {OT_7AgAAQBAJ},
  eprinttype = {googlebooks},
  publisher = {{University of Toronto Press}},
  abstract = {What, other than numbers and power, justifies Canada\&\#39;s assertion of sovereignty and jurisdiction over the country\&\#39;s vast territory? Why should Canada\&\#39;s original inhabitants have to ask for rights to what was their land when non-Aboriginal people first arrived? The question lurks behind every court judgment on Indigenous rights, every demand that treaty obligations be fulfilled, and every land-claims negotiation.Addressing these questions has occupied anthropologist Michael Asch for nearly thirty years. InOn Being Here to Stay, Asch retells the story of Canada with a focus on the relationship between First Nations and settlers.Asch proposes a way forward based on respecting the spirit and intent of treaties negotiated at the time of Confederation, through which, he argues, First Nations and settlers can establish an ethical way for both communities to be here to stay.},
  isbn = {978-1-4426-1002-6},
