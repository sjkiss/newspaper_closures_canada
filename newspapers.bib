@article{nisbetCompetitionWorldviewsValues2005,
  title = {The Competition for Worldviews: {{Values}}, Information, and Public Support for Stem Cell Research},
  author = {Nisbet, M},
  date = {2005-01-01},
  journaltitle = {International Journal of Public Opinion Research},
  shortjournal = {International Journal of Public Opinion Research},
  volume = {17},
